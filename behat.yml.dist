default:
    suites:
        organization:
            contexts:
                - Organization\Behat\OrganizationDomainContext
            filters:
                tags: "@organization"
        talk:
            contexts:
                - Talk\Behat\TalkDomainContext
            filters:
                tags: "@talk"
        user:
            contexts:
                - User\Behat\UserDomainContext
            filters:
                tags: "@user"
        user_app:
            contexts:
                - User\Behat\UserApplicationContext
            filters:
                tags: "@user && @application"
        event:
            contexts:
                - Event\Behat\EventDomainContext
            filters:
                tags: "@event"